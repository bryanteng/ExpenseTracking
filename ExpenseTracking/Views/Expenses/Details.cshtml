@model ExpenseTracking.Models.cf.Expense

@{
    Layout = "~/Views/Shared/_ExpenseLayout.cshtml";}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Details</title>
</head>
<body>
    <div>
        <h4>Expense</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                Expense
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.ExpenseName)
            </dd>
    
            <dt>
                Expense Value
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.ExpenseValue)
            </dd>
    
            <dt>
                @Html.DisplayNameFor(model => model.Date)
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.DateString)
            </dd>
    
        </dl>
    </div>
    <p>
        @Html.ActionLink("Edit", "Edit", new { id = Model.ExpenseId }) |
        @Html.ActionLink("Back to List", "Index")
    </p>
</body>
</html>
